<h1>New task</h1>

<form action="/coucou" method="post">
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <ul>
    <li><lable for="task">Title<label></li>
    <li><input type="string" name="task[title]"></li>
    <br>
    <li><lable for="details">Details<label></li>
    <li><input type="text" name="task[details]"></li>
    <br>
    <li><input type="submit" value="Send"></li>
  </ul>
</form>
